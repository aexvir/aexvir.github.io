	<!-- Loading Script -->
	<script>
		TweenMax.to($('#logoav img'),4,{delay:0.5,ease: Elastic. easeOut.config( 1.5, 0.25),rotationX:0});
		prbar = progressJs().start().autoIncrease(5, 1);
		prbar.onprogress(function(target, percent) {
			if(percent == 100){
                prbar.end();
				TweenMax.to($('#menu'),0.5,{display:'flex',ease: Sine.easeOut,top:0});
			}
		});
        $(document).ready(setTimeout(function(){prbar.set(100);},3000));
	</script>
